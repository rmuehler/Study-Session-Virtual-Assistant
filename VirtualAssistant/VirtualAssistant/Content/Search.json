{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.0",
  "body": [
    {
      "type": "TextBlock",
      "size": "Medium",
      "weight": "Bolder",
      "text": "Search",
      "horizontalAlignment": "Center"
    },
    {
      "type": "Input.Text",
      "placeholder": "Tutor",
      "value": "Tutor Placeholder",
      "style": "text",
      "maxLength": 0,
      "id": "StudentVal"
    },
    {
      "type": "Input.Text",
      "placeholder": "Subject",
      "value": "Subject Placeholder",
      "style": "Email",
      "maxLength": 0,
      "id": "StudentEmailVal"
    },
    {
      "type": "Input.Text",
      "placeholder": "Data",
      "value": "Data Placeholder",
      "id": "StudentPhoneVal"
    }
  ],
  "actions": [
    {
      "type": "Action.Submit",
      "title": "Search",
      "data": {
        "id": "searchCard"
      },
      "id": "searchPressed",
      "style": "positive"
    }
  ]
}